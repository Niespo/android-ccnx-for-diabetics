<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="pl.androidland.studia.tirt.diabetichelper">

    <application
            android:allowBackup="true"
            android:icon="@drawable/ic_launcher"
            android:label="@string/app_name"
            android:theme="@style/_DiabeticsHelperTheme"
            android:name=".ApplicationBus">
        <activity
                android:name=".ui.android.components.activities.MainActivity"
                android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".ui.android.components.activities.RegisterActivity"
                android:label="@string/app_name">
        </activity>
        <activity
                android:name=".ui.android.components.activities.PatientActivity"
                android:label="@string/app_name">
        </activity>
        <activity
                android:name=".ui.android.components.activities.DoctorActivity"
                android:label="@string/app_name">
        </activity>
        <activity
                android:name=".ui.android.components.activities.MeasurementActivity"
                android:label="@string/app_name">
        </activity>
    </application>

</manifest>
